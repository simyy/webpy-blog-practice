<!-- __base.html__ -->
<html>
    <head lang="en">
        <meta charset="UTF-8">
		{% block meta %}<!-- block meta  -->{% endblock %}
		<title>{% block title %}iFancy{% endblock %}</title>/>
		<link href="static/css/bootstrap.css" rel="stylesheet">
		<link href="static/css/style.css" rel="stylesheet">
        <link href="static/css/base.css" rel="stylesheet">
        <link href="static/css/animate.css" rel="stylesheet">
		{% block beforehead %}<!-- before head  -->{% endblock %}
    </head>
	
    <body>
        <!-- Part 1: Wrap all page content here -->
		<div id="wrap">
            <header id="header" class="header header--fixed hide-from-print animated slideDown" role="banner">
                <div class="container">
                    <nav id="nav" class="nav-wrapper" role="navigation">
                        <ul class="nav nav--main">
                            <li class="nav__item ">
                                <a class="header__link subdued " href="#">
                                    <!--span aria-hidden="true" class="icon icon--github"></span-->
                                    <i class="icon-bold icon-white"></i>
                                     <span class="complimentary push--left">Blog</span>
                                </a>
                            </li>
                            <li class="nav__item">
                                <!-- Modal -->
                                <div id="myModal" class="modal hide fade" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
                                    <div class="modal-header">
                                        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
                                        <h3 id="myModalLabel">Modal header</h3>
                                    </div>
                                    <div class="modal-body"><p>One fine body…</p></div>
                                    <div class="modal-footer">
                                        <button class="btn" data-dismiss="modal" aria-hidden="true">关闭</button>
                                        <button class="btn btn-primary">Save changes</button>
                                    </div>
                                </div>
                                <a class="header__link subdued" role="button" href="#myModal">
                                    <!--span aria-hidden="true" class="icon icon--twitter"></span-->
                                    <i class="icon-envelope icon-white"></i>
                                    <span class="complimentary push--left" data-toggle="modal" data-target="#myModal">contact</span>
                                </a>
                            </li>
                        </ul>
                    </nav>
                    <a href="../" class="brand header__link">
                        <b class="brand__forename">iFancy&nbsp;</b><b class="muted credit">爱幻想</b>
                    </a>
                </div>
            </header>


            <!-- Begin page content -->
            <div style="padding-top:40px;">
                {% block content %}
                <div class="page-header">
                    <h1>Sticky footer with fixed navbar</h1>
                </div>
                <p class="lead">Pin a fixed-height footer to the bottom of the viewport in desktop browsers with this custom HTML and CSS. A fixed navbar has been added within <code>#wrap</code> with <code>padding-top: 60px;</code> on the <code>.container</code>.</p>
                <p>Back to <a href="./sticky-footer.html">the sticky footer</a> minus the navbar.</p>
                {% endblock %}
            </div>

            <div id="push"></div>
        </div>

        <div id="footer">
            <div class="container">
                <div >
                    <div style="float:left;"><p class="muted credit">&nbsp;&nbsp;&nbsp;幻想，因为还年轻</p></div>
                    <div style="float:right;"><img src="static/img/tubiao.png" height="50" width="50"/></div>
                    <div style="float:right;"><p class="muted credit">iFancy.cc by cococo点点&nbsp;&nbsp;</p></div>
                </div>
            </div>
        </div>
		<!-- jQuery (Bootstrap  JavaScript jQuery) -->
		<script src="static/js/jquery.min.js"></script>
		<script src="static/js/bootstrap.js"></script>
        <!--script src="static/js/jquery.vticker.min.js"></script-->
        <script src="static/js/headroom.js"></script>
        {% block script %}{% endblock %}
        <script type="text/javascript">
        $(function() {
            var header = new Headroom(document.querySelector("#header"), {
                tolerance: 15,
                offset : 85,
                classes: {
                  initial: "animated",
                  pinned: "slideDown",
                  unpinned: "slideUp"
                }
            });
            header.init();

            var bttHeadroom = new Headroom(document.getElementById("btt"), {
                tolerance : 0,
                offset : 500,
                classes : {
                    initial : "slide",
                    pinned : "slide--reset",
                    unpinned : "slide--down"
                }
            });
            bttHeadroom.init();
        }());
        $('#myModal').modal(options)
        </script>
    </body>
</html>